    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "serve": "^14.2.0"
  },
  "dependencies": {
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2"
  }
